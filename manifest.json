{
  "manifest_version": 3,
  "name": "Weather Forcast",
  "description": "Base Level Extension",
  "version": "0.1",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.jpeg"
  },
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["https://www.google.com/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://*/", "https://*/*", "http://*/"],
      "js": ["content.js"]
    }
  ]
}
