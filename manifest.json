{
  "manifest_version": 3,
  "name": "Weather Report",
  "description": "Base Level Extension",
  "version": "0.1",
    "default_popup": "html",
    "default_icon": "png"
  },
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["https://www.google.com/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://*/", "https://*/*", "http://*/"],
      "js": ["content.js"]
    }
  ]
}
